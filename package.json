    "@types/compression": "^1.7.5",
    "@types/graphql-upload": "^16.0.7",
    "@types/node": "^22.5.5",
    "glob": "^11.0.0",
    "request": "^2.88.2",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "unzipper": "^0.10.11",
    "yargs": "^17.7.2"
  }

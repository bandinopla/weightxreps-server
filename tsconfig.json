{
    "extends":"@tsconfig/node18",
    "compilerOptions": {
        "moduleResolution":"node",
        "module":"CommonJS",
        "allowJs": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "outDir": "./server-to-upload",
        "noImplicitAny":false
    },
    "include": ["server/**/*"],
    "exclude": ["node_modules" ]
}